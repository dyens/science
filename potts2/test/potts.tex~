  \fmf{fermion,tension=.4}{v1,v2}
  \fmf{fermion,tension=3}{t,v3}
   \end{fmfgraph*}
   \end{fmffile}}}
   +\parbox[c]{25mm}{\center\begin{fmffile}{ddg7gf0}\begin{fmfgraph*}(20,35)
  \fmfright{r}
  \fmfleft{l}
  \fmftop{t}
  \fmfdot{v1,v2,v3}
    \fmf{plain,tension=3}{v2,r}
  \fmf{plain,tension=3}{v1,l}
